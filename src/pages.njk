      overflow: hidden;
      cursor: pointer;
      text-transform: uppercase;
      letter-spacing: 1px;
    }
    
    .features {
      padding: 40px 15px;
    }
    
    .features-title {
      text-align: center;
      font-size: 28px;
      margin-bottom: 30px;
      background: linear-gradient(135deg, #FFD700 0%, #FFA500 100%);
      -webkit-background-clip: text;
      -webkit-text-fill-color: transparent;
      background-clip: text;
    }
    
    .features-grid {
      display: grid;
      grid-template-columns: 1fr;
      gap: 20px;
      max-width: 800px;
      margin: 0 auto;
    }
    
    .feature-card {
